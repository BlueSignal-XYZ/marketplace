{
  "name": "neptunechain-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wqt && npm run build:cloud && npm run build:sales",
    "preview": "vite preview",
    "build:wqt": "BUILD_TARGET=wqt vite build --outDir dist-wqt && bash scripts/copy-seo-files.sh wqt",
    "build:cloud": "BUILD_TARGET=cloud vite build --outDir dist-cloud && mv dist-cloud/cloud.html dist-cloud/index.html 2>/dev/null || true && bash scripts/copy-seo-files.sh cloud",
    "build:sales": "BUILD_TARGET=sales vite build --outDir dist-sales && mv dist-sales/sales.html dist-sales/index.html 2>/dev/null || true && bash scripts/copy-seo-files.sh sales",
    "deploy:wqt": "npm run build:wqt && firebase deploy --only hosting:waterquality-trading",
    "deploy:cloud": "npm run build:cloud && firebase deploy --only hosting:cloud-bluesignal",
    "deploy:sales": "npm run build:sales && firebase deploy --only hosting:sales-bluesignal",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@livepeer/react": "^2.8.6",
    "@lottiefiles/react-lottie-player": "^3.6.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@react-google-maps/api": "^2.19.2",
    "@stripe/react-stripe-js": "^2.1.1",
    "@stripe/stripe-js": "^1.54.2",
    "@tanstack/react-query": "^4.29.23",
    "@types/three": "^0.157.2",
    "alchemy-sdk": "^3.3.1",
    "axios": "^1.5.0",
    "chart.js": "^4.4.1",
    "crypto-js": "^4.1.1",
    "ethers": "^6.8.1",
    "firebase": "^10.8.1",
    "font-awesome": "^4.7.0",
    "framer": "^2.3.0",
    "framer-motion": "^10.16.4",
    "http-proxy-middleware": "^3.0.0",
    "js-cookie": "^3.0.5",
    "mapbox-gl": "^2.15.0",
    "moralis": "^2.23.2",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-loader-spinner": "^5.4.5",
    "react-lottie": "^1.2.4",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.15.0",
    "react-spring": "^9.7.3",
    "react-tabs": "^6.0.2",
    "react-tooltip": "^5.21.4",
    "styled-components": "^6.0.7",
    "three": "^0.157.0",
    "tus-js-client": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
